@inproceedings{Tonietti2009b,
  title = {De l’ama-gal en d’Ebla à la Valide Sultan ottomane. L’importance de la ‘mère du roi’ dans le Proche-Orient depuis la plus haute antiquité},
  shorttitle = {De l’ama-gal en d’Ebla à la Valide Sultan},
  booktitle = {Et il y eut un esprit dans l'homme: Jean Bottéro et la Mésopotamie},
  author = {Tonietti, Maria Vittoria},
  editor = {Faivre, Xavier and Lion, Brigitte and Michel, Cécile},
  date = {2009},
  series = {Travaux de la Maison René-Ginouvès 6},
  pages = {261--274},
  publisher = {De Boccard},
  location = {Paris},
  isbn = {978-2-7018-0261-9},
  langid = {french},
  keywords = {Women at Ebla},
  note = {\newline☞ The paper by Tonietti explores the significant influence of women in Syro-Mesopotamian culture, specifically focusing on their political power and the role of queen mothers in the kingdom of Ebla. While women were generally denied direct access to political authority, they were able to exert strong influence through their husbands or sons. The paper highlights the historical importance of Dusigu, the mother of the king of Ebla in the mid-24th century B.C., as the first woman in ancient Near Eastern history to initiate a tradition that continued through the centuries to the Ottoman Empire.
\par
Tonietti acknowledges the extensive information available on the king's mother, known as the ama-gal-en, in the works published to date on the women of the Ebla palace. Recently published texts have provided new insights into the ArruLUM period, which was previously poorly documented, and have proposed a new chronological framework for the succession of the last three sovereigns and their respective ministers. With this updated information, the paper delves deeper into the role of the ama-gal-en, exploring various aspects closely linked to significant moments in the history of the kingdom of Ebla.},
  file = {/Users/ericascarpa/Bookshelf/Tonietti2009b - De l’ama-gal en d’Ebla à la Valide Sultan ottomane - Travaux de la Maison René-Ginouvès 6 - EB2541.pdf}
}

