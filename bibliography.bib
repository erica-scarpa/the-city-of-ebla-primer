  keywords = {Animals at Ebla},
  file = {/Users/ericascarpa/Bookshelf/BennettEtAl2022 - The Genetic Identity of the Earliest Human-Made Hybrid Animals, the kungas of - Sci. Adv. 8 - EB2900.pdf}
