@inproceedings{Archi1996e,
  title = {Les comptes rendus annuels de métaux (CAM)},
  shorttitle = {Comptes rendus},
  booktitle = {Mari, Ébla et les Hourrites. Dix ans de travaux, Première Partie. Actes du colloque international (Paris, mai 1993)},
  author = {Archi, Alfonso},
  editor = {Durand, Jean-Marie},
  date = {1996},
  pages = {73--99},
  location = {Paris},
  langid = {french},
  note = {\newline☞ The article by Archi discusses a collection of large tablets found at Ebla, which are similar in shape to monthly textiles distribution. These tablets, referred to as the annual accounts of silver and gold are date to different periods. The tablets from the first phase, attributed to minister Ibrium, have fifteen to nineteen columns on both sides. As Ebla's economic power increased under Ibbi-Zikir, the tablets grew larger, exceeding twenty columns. The accounts show a significant progression in silver quantities over time. For example, one tablet records an outflow of 100.49 mines of silver, while another reports an outflow of 273.46 silver minas and 437 mines from the treasury. The tablets also occasionally provide information related to chronology. Some colophons mention the passage of time, such as "2 mu" or "x mu é en diri." The annual nature of these documents is further demonstrated by periodic cult offerings, including those for the god Hadda in the third and eighth months, as well as the presence of two bracelets for the gods Ammarig and Ašdabil in all complete texts.
\par
The longest documents in the collection refer to events that span several months, although the chronological order is not always maintained. The AAMs serve as a supporting backbone for arranging the remaining epigraphic material in chronological order.},
  file = {/Users/ericascarpa/Bookshelf/Archi1996e - Les comptes rendus annuels de métaux (CAM) - Amurru 1 - EB0181.pdf}
}

