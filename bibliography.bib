@inproceedings{Steinkeller2021a,
  title = {International {{Trade}} in {{Greater Mesopotamia During Late Pre-Sargonic Times}}: {{The Case}} of {{Ebla}} as {{Illustrated}} by {{Her Participation}} in the {{Euphratean Timber Trade}}},
