@book{Vacca2020,
  title = {The {{Early Bronze Age III}} and {{IVA1}} at {{Tell Mardikh-Ebla}} and {{Its Region}}: {{Stratigraphic}} and {{Ceramic Sequences}}},
  shorttitle = {The {{Early Bronze Age Iii}} and {{Iva1}} at {{Tell Mardikh-Ebla}} and {{Its Region}}},
