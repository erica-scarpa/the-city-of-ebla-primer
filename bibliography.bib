@incollection{Pinnock2021d,
  title = {In the {{Light}} and in the {{Dark}}: {{Exhibiting Power}}, {{Exploiting Spaces}} in {{Early}} and {{Old Syrian Ebla}}—an {{Analysis}} of the {{Five Senses}} in an {{Early}}–{{Old Syrian Court}}},
  shorttitle = {In the {{Light}} and in the {{Dark}}},
  booktitle = {The {{Routledge Handbook}} of the {{Senses}} in the {{Ancient Near East}}},
  author = {Pinnock, Frances},
  editor = {Neumann, Kiersten and Thomason, Allison},
  date = {2021},
  publisher = {Routledge},
  abstract = {Recent studies have thrown light on the complex ceremonial activities of kingship in pre-classical Syria, with a special focus on Ebla, where the archaeological and written evidence from the Early Bronze IVA Age has contributed to a better understanding of several aspects of the ideology and manifestation of kingship. In a traditional perspective, these studies have focused on the analysis of architecture, clothing, and arts using the textual evidence as a support for interpretations. In these analyses, when dealing with the agency of the images and their surroundings, if sensorial experiences were mentioned, it was the sense of sight that was privileged. In light of the most recent approaches to the study of the ancient world, in this chapter takes a multisensory approach to the archaeological and textual evidence, reconstructing the activities, images, and words spoken in order to recreate the complex sensorial experience of ceremonial activities at Ebla during the Early Bronze IVA Age. It also takes into account some ceremonies that took place at Ebla in Middle Bronze I–II; for this later period, too, it is possible—albeit in a more speculative way—to reconstruct the complex sensescape of these public events.},
  isbn = {978-0-429-28020-7},
  pagetotal = {23},
  keywords = {Lacks details}
}

